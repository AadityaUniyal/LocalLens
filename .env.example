# Neon Database Configuration for Local Lens
# Copy this file to .env and update with your actual Neon credentials

# Neon Project Configuration
NEON_PROJECT_ID=your-project-id-here
NEON_DATABASE_URL=postgresql://username:password@hostname:5432/neondb?sslmode=require

# Environment
NODE_ENV=development

# Branch-specific Connection Strings
AUTH_SERVICE_DB_URL=postgresql://username:password@hostname:5432/neondb?sslmode=require
BLOOD_PLATFORM_DB_URL=postgresql://username:password@hostname:5432/neondb?sslmode=require
COMPLAINT_PLATFORM_DB_URL=postgresql://username:password@hostname:5432/neondb?sslmode=require
TRAFFIC_PLATFORM_DB_URL=postgresql://username:password@hostname:5432/neondb?sslmode=require

# Testing Configuration
TEST_DATABASE_URL=postgresql://test:test@localhost:5432/test_db
VERBOSE_TESTS=false