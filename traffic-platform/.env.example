FLASK_ENV=development
FLASK_PORT=5000

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5436/local_lens_traffic

# Redis
REDIS_URL=redis://localhost:6379

# Auth Service
AUTH_SERVICE_URL=http://localhost:3001

# Detection Parameters
DETECTION_CONFIDENCE_THRESHOLD=0.85
EMERGENCY_VEHICLE_COLORS=blue,red,white
MODEL_PATH=models/yolov8n.pt

# Dehradun Map Integration
MAPS_API_KEY=your_maps_api_key
CITY_BOUNDS_LAT_MIN=30.2500
CITY_BOUNDS_LAT_MAX=30.3500
CITY_BOUNDS_LNG_MIN=78.0000
CITY_BOUNDS_LNG_MAX=78.1000

# Signal Control
DEFAULT_GREEN_DURATION=30
DEFAULT_RED_DURATION=45
EMERGENCY_OVERRIDE_DURATION=60